<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="100dp">

    <TextView
        android:id="@+id/tv_result_item_title"
        android:layout_width="wrap_content"
        android:text="11111111111111"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:layout_marginStart="38dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp" />

    <TextView
        android:id="@+id/tv_result_item_from"
        android:textColor="@color/red"
        android:text="22222"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="41dp"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/tv_result_item_time"
        android:layout_marginBottom="21dp" />

    <TextView
        android:id="@+id/tv_result_item_time"
        android:text="33333"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@android:color/darker_gray"
        android:layout_alignTop="@+id/tv_result_item_from"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="33dp" />

</RelativeLayout>
